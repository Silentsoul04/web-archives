}</p>

<p><br />
rule Malwareusedbycyberthreatactor2<br />
{<br />
    strings:<br />
        $str1 = "_quit"<br />
        $str2 = "_exe"<br />
        $str3 = "_put"<br />
        $str4 = "_got"<br />
        $str5 = "_get"<br />
        $str6 ="_del"<br />
